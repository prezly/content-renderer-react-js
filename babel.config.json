{
    "presets": [
        "@babel/react",
        "@babel/typescript",
        [
            "@babel/env",
            {
                "targets": {
                    "esmodules": false
                },
                "modules": "commonjs"
            }
        ]
    ],
    "overrides": [{
        "test": "./node_modules/@prezly/**/*.mjs",
        "plugins": [
            ["@babel/plugin-transform-modules-commonjs", {"importInterop": "none", "strict": true}]
        ]
    }]
}
